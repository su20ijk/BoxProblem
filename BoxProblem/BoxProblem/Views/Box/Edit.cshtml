
@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<div class="row">
    <div class="offset-2">
        <p>Weight: </p><input placeholder="current" />
    </div>
</div>
<div class="row">
    <div class="offset-2">
        <p>Volume: </p><input placeholder="current"/>
    </div>
</div>
<div class="row">
    <div class="offset-2">
        <p>Liquid(Yes or No): </p><input placeholder="current" />
    </div>
</div>
<div class="row">
    <div class="offset-2">
        <p>Cost: </p><input placeholder="current"/>
    </div>
</div>
<div class="row">
    <div class="offset-2">
        <p>Amount in Inventory: </p><input placeholder="current"/>
    </div>
</div>
<div class="row">
    <div class="offset-6">
        <button>Save</button>
        <button>Back</button>
    </div>
</div>